<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="CircularDeterminateProgressBarUser">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progresscircle</item>
    </style>
    <style name="CircularDeterminateProgressBarKyle">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/kyle_progress_circle</item>
    </style>

</resources>